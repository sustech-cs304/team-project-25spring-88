{
    "metadata": [
        {
            "src": [
            {
                "src": "..",
                "files": [
                    "Assets/CommonScripts/**/*.cs",
                    "Assets/Editor/**/*.cs",
                    "Assets/GameSystem/**/*.cs",
                    "Assets/Landing/Scripts/**/*.cs",
                    "Assets/Monoplayer/Scripts/**/*.cs",
                    "Assets/Multiplayer/Scripts/**/*.cs"
                ],
                "exclude": [
                    "**/*.Tests.cs",
                    "**/Mirror/**",
                    "**/kcp2k/**",
                    "**/IO.Swagger/**"
                ]
            }
        ],
            "globalNamespaceId": "Global",
            "filter": "filterConfig.yml",
            "dest": "api"
        }
    ],
    "build": {
        "template": ["default"],
        "exportApiNamespaceToc": true,
        "globalMetadata": {
            "_appTitle": "SUSTech Racing Car Documentation",
            "_appFooter": "Generated by DocFX",            
            "_appLandingPage": "index.html",
            "_namespaceLayout": "NamespaceLayout.html"
        },
        "content": [
            {
                "src": "api",
                "files": [
                    "*.yml"
                ],
                "dest": "./"
            }
        ],
        "overwrite": [
            {
                "src": "..",
                "files": [
                    "Assets/Scripts/**/*.md"
                ]
            }
        ],
        "resource": [
            {
                "files": [
                    "resources/**/*"
                ]
            }
        ],
        "sitemap": {
            "baseUrl": "https://normanderwan.github.io/DocFxForUnity",
            "changefreq": "weekly",
            "fileOptions": {
                "api/*": {
                    "changefreq": "daily"
                }
            }
        },
        "xref": [
            "https://normanderwan.github.io/UnityXrefMaps/xrefmap.yml"
        ],
        "xrefService": [
            "https://xref.docs.microsoft.com/query?uid={uid}"
        ],
        "dest": "../_site",
        "homepage": "index.html"
    }
}